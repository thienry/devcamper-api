{
  "name": "devcamper-api",
  "version": "0.0.1-alpha",
  "description": "A backend API for devcamper app",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "prepare": "husky install",
    "lintstaged": "lint-staged",
    "livedoc": "npx http-server docs/",
    "docgen": "jsdoc -c jsdoc.json",
    "lint:fix": "eslint --fix --ext .js src/",
    "test": "jest --colors --noStackTrace --passWithNoTests --runInBand",
    "test:unit": "npm test -- --watch -c src/config/jest-unit.js",
    "test:integration": "npm test -- --watch -c src/config/jest-integration.js",
    "test:staged": "npm test -- --findRelatedTests --silent",
    "test:ci": "npm test -- --coverage --silent",
    "test:coveralls": "npm run test:ci && coveralls < coverage/lcov.info"
  },
  "keywords": [],
  "author": "Thiago Moura <thmoura14@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "chalk": "^3.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.12.5",
    "morgan": "^1.10.0",
    "slugify": "^1.5.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "^1.2.4",
    "@sucrase/jest-plugin": "^2.1.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-jest": "^24.3.5",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "jsdoc": "^3.6.6",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "sucrase": "^3.18.1",
    "supertest": "^6.1.3"
  }
}
